{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": 1,
    "react/prop-types": 0
  }
}
